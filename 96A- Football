#include<iostream>
#include<stdio.h>
#include<cstring>
#include<algorithm>
using namespace std;

int main()
 {
   string s;
   cin>>s;
   int check=0;
   int l=s.length();
   for(int i=0;i<l-6;i++)
   {
       if(s[i]=='1'&& s[i+1]=='1'&& s[i+2]=='1'&& s[i+3]=='1'&& s[i+4]=='1'&& s[i+5]=='1'&& s[i+6]=='1')
       {
           cout<<"YES";
           check=1;
           break;
       }
       if(s[i]=='0' && s[i+1]=='0'&& s[i+2]=='0'&& s[i+3]=='0'&& s[i+4]=='0'&& s[i+5]=='0'&& s[i+6]=='0')
       {
           cout<<"YES";
           check=1;
            break;
       }
   
       
   }
       if(check!=1)
       cout<<"NO";
   
   
 }
     
     
  
  
